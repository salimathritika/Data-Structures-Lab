#include<stdio.h>
#define size 10
int front=-1,rear=-1,a[size];

void insert(int ele)
{
    if(rear==(size-1))
        printf("\nQueue is full\n");
    else
        a[++rear]=ele;
}

int del()
{
    if(front==rear)
    {
        printf("\nQueue is empty\n");
        return (-999);
    }
    else
    {
        front=front+1;
        return(a[front]);
    }
}

void display()
{
    int i;
    if(front==rear)
        printf("\nQueue is empty\n");
    else
    {
        for(i=front+1;i<=rear;i++)
            printf("%d ",a[i]);
    }
}

int main()
{
    int a,ele;
    printf("1.To insert element\n2.To delete\n3.To display\n4.To exit\n");
    do
    {
        printf("\nChoice: ");
        scanf("%d",&a);
        switch(a)
        {
        case 1:
            {
                printf("Enter element:");
                scanf("%d",&ele);
                insert(ele);
                break;
            }
        case 2:
            {
                ele=del();
                if(del!=(-999))
                    printf("Deleted element is %d",ele);
                break;
            }
        case 3:
            {
                display();
            }
        }
    }while(a!=4);
}
